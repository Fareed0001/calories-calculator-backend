<%- include("partials/header"); -%>

  <div class="contact-content">
    <div class="contact-image-div">
      <a href="/"><img class="contact-image" src="images\logo.png" alt="contact-image"></a>
      <p class="contact-header">Leave a message and we will get back to you as soon as possible.</p>
    </div>
    <div class="form container">
      <form class="row g-3 needs-validation" novalidate action="mailto:faridibrahim0003@gmail.com" method="post" enctype="text/plain">
        <div class="col-sm-12">
          <div class="row">
            <div class="col-md input-group-lg"> <label for="validationCustom01" class="form-label contact-text">Name</label> <input type="text" class="form-control" name="Name " id="validationCustom01" value="" required>
              <div class="valid-feedback"> Looks good! </div>
            </div>
            <div class="col-md input-group-lg"> <label class="form-label contact-text" for="exampleInputEmail1">Email</label> <input class="form-control" name="Email " id="emailAddress" aria-describedby="emailHelp" type="email" placeholder="" />
              <div class="valid-feedback"> Looks good! </div>
            </div>
          </div>
        </div>
        <div class="col-sm-12 message-box input-group-lg"> <label for="validationTextarea" class="form-label contact-text">Message</label> <textarea name="Message" rows="5" class="form-control is-invalid" id="validationTextarea" placeholder=""
            required>
           </textarea>
        </div>
        <div class="col-sm-12 text-center contact-button">
          <button type="submit" class="btn mx-auto rounded-pill btn-lg btn-outline-primary">Submit Message</button>
        </div>
      </form>
    </div>
  </div>

<%- include("partials/footer"); -%>